<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net8.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UseWPF>true</UseWPF>
    <ApplicationIcon>bumblebee.ico</ApplicationIcon>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Fonts\Lobster-Regular.ttf" />
    <None Remove="Images\background.jpg" />
    <None Remove="Images\galaxy.jpg" />
    <None Remove="Images\plant.png" />
    <None Remove="Images\zombie ball.png" />
    <None Remove="Images\zombie.png" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="bumblebee.ico" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Fonts\Lobster-Regular.ttf" />
    <Resource Include="Images\background.jpg" />
    <Resource Include="Images\galaxy.jpg" />
    <Resource Include="Images\plant.png" />
    <Resource Include="Images\zombie ball.png" />
    <Resource Include="Images\zombie.png" />
  </ItemGroup>

</Project>
